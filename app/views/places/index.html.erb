<div class="pagination">
  <%= will_paginate @places , previous_label: h("< Previous"), next_label: h("Next >") %>
</div>
<% @places.each do |place| %>
  <div class="booyah-box" col-10 offset-1>
    <h1><%= place.name %></h1><br />
    <i><%= place.address %></i><br /><br />
    <p><%= place.description %></p><br />
  
  </div>

  <hr />

<% end %>